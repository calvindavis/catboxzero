{
	"manifest_version": 2,

	"name": "Catbox Zero",
	"description": "Reward yourself for your empty inbox with a fun cat GIF!",
	"version": "1.0.0",
	"permissions": ["storage"],

	"content_scripts": [
		{
			"matches": ["https://mail.google.com/*"],
			"js": ["script.js"]
		}
	],

	"browser_action": {
		"default_popup": "options.html"
	}


}
